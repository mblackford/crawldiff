{
  "name": "crawldiff",
  "version": "1.0.5",
  "description": "Crawls a website and compares each page to a different host.",
  "repository": {
    "url": "https://github.com/mblackford/crawldiff"
  },
  "files": [
    "/lib"
  ],
  "main": "./lib/index.js",
  "bin": "./lib/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
