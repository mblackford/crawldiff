    "release": "npm run build && np"
  },
  "author": "Matthew Blackford",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "cli-diff": "^1.0.0",
    "cli-progress": "^3.8.2",
    "cli-table": "^0.3.1",
    "commander": "^6.2.0",
    "figlet": "^1.5.0",
    "jsdom": "^16.4.0",
    "node-fetch": "^3.1.1",
    "string-similarity": "^4.0.3",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.8.0",
    "@types/cli-table": "^0.3.0",
    "@types/jsdom": "^16.2.5",
    "@types/node": "^14.14.8",
    "@types/node-fetch": "^2.5.7",
    "@types/string-similarity": "^4.0.0",
    "@types/url-parse": "^1.4.3",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5",
    "np": "^7.0.0"
